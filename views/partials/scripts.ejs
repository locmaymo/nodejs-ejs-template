<script>
    <% if (successMessages && successMessages.length > 0) { %>
      <% successMessages.forEach(function(msg) { %>
        Swal.fire({
          icon: 'success',
          title: '<%= msg %>',
          position: 'top-end',
          toast: true,
          showConfirmButton: false,
          timer: 3000
        });
      <% }); %>
    <% } %>

    <% if (errorMessages && errorMessages.length > 0) { %>
      <% errorMessages.forEach(function(msg) { %>
        Swal.fire({
          icon: 'error',
          title: '<%= msg %>',
          position: 'top-end',
          toast: true,
          showConfirmButton: false,
          timer: 3000
        });
      <% }); %>
    <% } %>
</script>


